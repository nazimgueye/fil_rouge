pipeline {
    agent any
    stages {
        stage('Build images') {
            steps {
                sh 'docker version'
            }
        }
        stage('Tag images') {
            steps {
               sh 'docker build -t test1-db:1.0 -f Db.Dockerfile .'
               sh 'docker build -t test1-php:1.0 -f Web.Dockerfile .'
            }
        }
    }
}
