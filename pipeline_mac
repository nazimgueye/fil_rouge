pipeline {
  agent any
  stages {
    stage ('test') {
      steps{
       sh 'docker version'

      }
    }
    stage ('Run Docker Compose') {
      steps{
        sh 'docker-compose up  -d'
      }
    }
            post {
        success {
            script {
                slackSend(channel: "jenkins", message: "my-first-pipeline-slack passed successfully")
            }
        }
      }
}
  
}
